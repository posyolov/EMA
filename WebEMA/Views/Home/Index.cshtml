@model IEnumerable<Repository.CatalogItem>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>HomeController</title>
</head>
<body>
    <table class="table">
        <tr><td>ID</td><td>Title</td><td>Vendor</td></tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.Vendor.Name</td>
            </tr>
        }
    </table>
</body>
</html>
